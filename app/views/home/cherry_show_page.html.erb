<section class="ft-sc ft-sc-banner px-5 py-8 md:py-10 lg:py-12 text-gray-600">
  <div class="container mx-auto">
    <div class="flex items-center flex-wrap">
      <div class="w-full lg:w-1/2 text-center lg:text-left">
        <p class="text-lg">Royal Ranch</p>
        <h1 class="text-3xl sm:text-4xl lg:text-5xl text-gray-800 font-extrabold">Farm fresh Cherry</h1>
        <p class="text-xl md:text-2xl leading-6">100% USA </p>
        <h4 class="text-xl mt-4 text-gray-800">Cherry Variety: <%= @cherry.variety %></h4>
        <p class="text-lg mt-4">
          <button type="button"
            class="text-primary-base"
            data-controller="openmodal"
            data-openmodal-selector=".vimeo-modal"
            data-action="click->openmodal#open">
              <i class="far fa-play-circle"></i> Learn about FarmTracer
          </button>
        </p>
        <h5 class="mt-4 text-lg">About the Farm</h5>
        <p class="lg:text-lg">Royal Ranch is a family farm in Central Washington. Central Washington’s mild climate is ideal for raising cattle and many other crops.</p>
        <p class="mt-6">
          <a href="https://www.royalfamilyfarming.com/" class="text-primary-base">www.RoyalFamilyFarming.com</a>
        </p>
      </div>
      <div class="w-full lg:w-1/2 text-center">
        <div class="relative inline-block">
          <%= image_tag('product-img-02.png') %>
        </div>
      </div>
    </div>
  </div>
</section>

