    <div class="bg-white rounded-xl flex-1 p-8">
      <div style="padding:100% 0 0 0;position:relative;">
        <iframe src="https://player.vimeo.com/video/483084891?title=0&byline=0&portrait=0&muted=1&autoplay=1" style="position:absolute;top:0;left:0;width:100%;height:100%;" frameborder="0" allow="autoplay; fullscreen" allowfullscreen data-target="modal.video"></iframe>
